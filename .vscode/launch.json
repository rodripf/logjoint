    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug chromium int tests on mac",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/trunk/sdk/tools/logjoint.plugintool/bin/Debug/netcoreapp2.1/dotnet-logjoint.plugintool.dll",
            "args": [
                "test",
                "${workspaceFolder}/trunk/extensions/chromium/plugin/bin/Debug/netstandard2.0",
                "${workspaceFolder}/trunk/platforms/osx/bin/debug/logjoint.app/Contents/MonoBundle",
                "--filter=*"
